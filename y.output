Grammar

    0 $accept: statement_list $end

    1 statement_list: statement_list statement ';' '\n'
    2               | %empty

    3 statement: select_statement

    4 expr: INTNUMBER
    5     | '(' expr ')'
    6     | expr '+' expr
    7     | expr '-' expr
    8     | expr '*' expr
    9     | expr '/' expr
   10     | %empty

   11 select_statement: SELECT select_options top_options

   12 select_options: %empty
   13               | select_options ALL
   14               | select_options DISTINCT

   15 top_options: %empty
   16            | top_options TOP expr PERCENT
   17            | top_options TOP expr


